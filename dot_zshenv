. "$HOME/.cargo/env"
. "$HOME/.llm.env"


# PATH updates
export PATH="$PATH:$HOME/go/bin"

# ENV vars

export EDITOR="/opt/homebrew/bin/nvim"
export ZSHRC="$HOME/.zshrc"
export ZSHENV="$HOME/.zshenv"

# aliases
alias llmos="llm -t oneshot"
alias ghwr="gh workflow run"
alias ghwls="gh workflow list"
alias ghgls="gh gist list"
alias ls="eza --classify --icons --hyperlink -t modified"
alias ll="ls -l"
